{
  "name": "clean-ui",
  "version": "1.0.0",
  "description": "Clean UI is a CSS theme that promotes clarity through the use of clean visuals",
  "scripts": {
    "start": "node index.js",
    "build": "node-sass --include-path scss scss/clean-ui.scss dist/css/clean-ui.css",
    "build-min": "node-sass --output-style compressed --include-path scss scss/clean-ui.scss dist/css/clean-ui.min.css",
    "watch": "npm run build; node-sass -w --include-path scss scss/clean-ui.scss dist/css/clean-ui.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Netflux/Clean-UI.git"
  },
  "keywords": [
    "clean",
    "ui",
    "css"
  ],
  "author": "Netflux",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Netflux/Clean-UI/issues"
  },
  "homepage": "https://github.com/Netflux/Clean-UI#readme",
  "dependencies": {},
  "devDependencies": {
    "chokidar": "^1.7.0",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9"
  }
}
